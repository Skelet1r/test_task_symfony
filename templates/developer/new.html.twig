
{% block title %}New Developer{% endblock %}

{% block body %}
    <h1>Create new Developer</h1>

    {{ include('developer/_form.html.twig') }}

    {% for label, messages in app.flashes %}
        <div class="alert alert-{{ label }}">
            {% for message in messages %}
                <p>{{ message }}</p>
            {% endfor %}
        </div>
    {% endfor %}

    <a href="{{ path('app_developer_index') }}">back to list</a>
{% endblock %}
