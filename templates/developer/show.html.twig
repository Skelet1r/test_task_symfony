
{% block title %}Developer{% endblock %}

{% block body %}
    <h1>Developer</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ developer.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ developer.name }}</td>
            </tr>
            <tr>
                <th>SurName</th>
                <td>{{ developer.surName }}</td>
            </tr>
            <tr>
                <th>MiddleName</th>
                <td>{{ developer.middleName }}</td>
            </tr>
            <tr>
                <th>JobTitle</th>
                <td>{{ developer.jobTitle }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ developer.email }}</td>
            </tr>
            <tr>
                <th>PhoneNumber</th>
                <td>{{ developer.phoneNumber }}</td>
            </tr>
            <tr>
                <th>Project</th>
                <td>{{ developer.project }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_developer_index') }}">back to list</a>

    <a href="{{ path('app_developer_edit', {'id': developer.id}) }}">edit</a>

    {{ include('developer/_delete_form.html.twig') }}
{% endblock %}
